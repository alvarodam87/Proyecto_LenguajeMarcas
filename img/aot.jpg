<script>

    new Vue({
        el: '#headerWrapper',
         data: {
            token:false,
            listNav:'',
            productUrl:"https://pixulovesk.click/product_details/",
            urlNav:"https://pixulovesk.click/category",
            searchData:{
                page:'1',
                num:'20',
                keywords:'',
            },
             searchDataShow:false,
            tokenTime:{
                token:'',
                expiration:'',
            },
            navShow:false,
            navListShow:false,
            countryShow:false,
            cartData:{
                list:[],
                num:0,
            },
             cartDataShow:false,
            cartListCheckbox:[],
            country:'',
             cartHove :false,
            countryImg:'https://pixulovesk.click/static/shopa/img/USD.png',
         },
        async created() {
            this.$http.get('/api/item/secondCate').then(function(response){
                if(response.data.code == 200){
                    this.listNav = response.data.data
                }else{
                    this.$toast({message: response.data.message,icon: 'warning-o',});
                }
           },function(res){
                this.$toast({message: res.data.message,icon: 'warning-o',});
            });
            var cookieArray=document.cookie.split("; ");
            for (var i=0;i<cookieArray.length;i++){
            var arr=cookieArray[i].split("=");
                if(arr[0]=="token")return this.cartFn(arr[1]);
            }

            this.cartFn();
        },
        watch:{
            'country': function (newVal,oldVal) {
                this.countryFn()
            },
        },
        methods: {
            countryShowFn(){
                if(this.countryShow){

            console.log('123')
                    this.countryShow = false
                }else{

            console.log('12')
                    this.countryShow = true
                }
            },
            countryFn(){
                this.$http.get('/api/setCurrency',{params : {country:this.country}}).then(function(response){
                    if(response.data.code == 200){
                        location.reload()
                    }else{
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                    }
               },function(res){
                    this.$toast({message: res.data.message,icon: 'warning-o',});
                });
            },
            submitFn:function(){
                if(this.cartListCheckbox.length>0){
                    window.event.returnValue = true;
                }else{
                    window.event.returnValue = false;
                    this.$toast({message: 'Please select the item you want to purchase',icon: 'warning-o'})
                }
            },
            cartRemove:function(row){
                let data = [];
                if(Array.isArray(row) == false){
                    data.push({
                        cart_id:row.cart_id,
                    })
                }else{
                    for(var i=0;i<row.length;i++){
                        data.push({
                            cart_id:row[i].cart_id,
                        })
                    }
                }
                this.$http.post('/api/cart/remove', {cart_str:data}, {headers : {Authorization:this.token}},{emulateJSON:true}).then(function(response){

                    if(response.data.code == 200){
                        window.location.href="https://pixulovesk.click/shopping_cart";
                    }else{
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                    }
                },function(res){

                    this.$toast({message: response.data.message,icon: 'warning-o',});

                });
            },
            //购物车
            cartFn:function(token){
                this.token = token;
                this.$http.get('/api/cart/index', {headers: {Authorization: token}}).then(async function (response) {
                    if(response.data.code == 200){
                        this.cartData = response.data.data;

                        if(this.cartData.list.length == 0){
                        }else{
                            //After loading the data, add all products in the data to the selected array



                            for(var i=0;i<this.cartData.list.length;i++){
                                await imgError(this.cartData.list[i].thumbnail , i , this.cartData.list,this);
                            }

                            this.cartListCheckbox= this.cartData.list.map((item)=>{

                                return item.cart_id
                            })

                            this.cartHove = true

                        }
                        this.cartDataShow = true;
                    }else{
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                    }

                }, function (res) {
                    this.$toast({message: response.data.message,icon: 'warning-o',});
                    window.location.href = "https://pixulovesk.click/login";
                });
                var key = ' ';
                if(key.length>1){
                    this.searchData.keywords = key;
                }
            },
            //搜索
            searchFn:function(){
                if(this.searchData.keywords){
                    this.$http.post('/api/item/search',this.searchData,{emulateJSON:true}).then(function(response){
                        if(response.data.code == 200){
                            if(response.data.data.total >= 1){
                                console.log("https://pixulovesk.click/advanced_search_result",1)
                                window.location.href="https://pixulovesk.click/advanced_search_result"+"?keywords="+encodeURIComponent(this.searchData.keywords)+"&page="+this.searchData.page+"&num="+this.searchData.num;
                            }else{
                                console.log("https://pixulovesk.click/advanced_search",2)
                                window.location.href="https://pixulovesk.click/advanced_search"+"?keywords="+this.searchData.keywords;
                            }
                            this.searchDataShow = true;
                        }else{
                            this.$toast({message: response.data.message,icon: 'warning-o',});
                        }
                    },function(res){
                        this.$toast({message: res.data.message,icon: 'warning-o',});
                    });
                }else{
                    console.log("https://pixulovesk.click/advanced_search",3)
                    window.location.href="https://pixulovesk.click/advanced_search"+"?keywords="+this.searchData.keywords;
                }
            },
            authorizationFn:function(){//get token
            console.log('123')
                var cookieArray=document.cookie.split("; ");
                for (var i=0;i<cookieArray.length;i++){
                  var arr=cookieArray[i].split("=");
                  if(arr[0]=="token")return this.logOut(arr[1]);
                }
            },
            logOut:function(token){
                this.$http.get('/api/logout',{headers : {Authorization:token}}).then(function(response){
                    if(response.data.code == 200){
                        document.cookie = "token=";
                        document.cookie="email=";
                        document.cookie="expiration=";
                        this.tokenTime = {
                                token:'',
                                expiration:'',
                            };
                        this.token = false;
                        window.location.href="https://pixulovesk.click/login";
                    }else{
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                    }
                },function(res){
                    this.$toast({message: res.data.message,icon: 'warning-o',});
                });
            },
            submitFn:function(){
                if(this.cartListCheckbox.length>0){
                    window.event.returnValue = true;
                }else{
                    window.event.returnValue = false;
                    this.$toast({message: 'Please select the item you want to purchase',icon: 'warning-o'})
                }
            },

        }

    })

</script>
