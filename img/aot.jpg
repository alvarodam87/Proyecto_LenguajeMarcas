    <script>
        let app=new Vue({

            el: '#navBreadCrumb_contentMainWrapper',

            data: {
                productData:'',
                productPageUrl:"https://pixulovesk.click/category",
                parentIDUrl:"https://pixulovesk.click/product_tag",
                url:"https://pixulovesk.click/product_details/",
                imgDag:'',
                featuredProductsList:[],
                productId: 110610800,
                productNum:1,
                buttonHove:false,
                productListShow:true,

                productValue:{
                },
                stab1:true,
                stab2:false,
                stab3:false,
                stabc1lass:'on',
                stabc2lass:'',
                stabc3lass:'',
                sizeShow:false,
                propertiesShow:false,
                titleShow:false,
                dateArr:[],
                data:[],
                productShow:false,
                stashNumber: null,
                watchNumber: null,
            },
            async created() {
                var data = {
                    num:10,
                }

                this.productDetailFn(110610800);
                this.generateRandomNumber();
            },
            watch: {

                productData(){

                }
            },
            methods: {
                generateRandomNumber() {
                    this.stashNumber = Math.floor(Math.random() * (50 - 10 + 1)) + 10;
                    this.watchNumber = Math.floor(Math.random() * (30 - 10 + 1)) + 10;
                },
                clearNoNum(value){
                    value = parseInt(value);
                    if(value<=1){
                        this.productNum = 1;

                    }else{
                        this.productNum = value;
                    }
                },
                productNumFn(value){
                    if(value){
                        this.productNum = this.productNum+1;
                    }else{
                        if(this.productNum <= 1){
                            this.productNum = 1;
                        }else{
                            this.productNum = this.productNum-1;
                        }

                    }
                },
                stabFn(index){
                    if(index==1){
                        this.stab1=true;
                        this.stab2=false;
                        this.stab3=false;
                        this.stabc1lass='on';
                        this.stabc2lass='';
                        this.stabc3lass=''
                    }else if(index==2){
                        this.stab1=false;
                        this.stab2=true;
                        this.stab3=false;
                        this.stabc1lass='';
                        this.stabc2lass='on';
                        this.stabc3lass=''
                    }else if(index==3){
                        this.stab1=false;
                        this.stab2=false;
                        this.stab3=true;
                        this.stabc1lass='';
                        this.stabc2lass='';
                        this.stabc3lass='on'
                    }
                },
                productDetailFn(id){
                    this.productId = decodeURIComponent(id)
                    // console.log('ID:', this.productId);
                    let that=this
                    //注释1，函数加了async，处理循环里的异步问题，
                    this.$http.get('/api/item/info',{params : {id:this.productId}}).then(async function(response){
                        if(response.data.code == 200){
                            this.productListShow = false;
                            this.productData = response.data.data;
                            this.productData.reviewLength = this.productData.review.length;
                            this.productData.score=Math.ceil(this.productData.score);

                            this.productData.price_html=this.productData.price_html.replace(/(\d+.\d+)|(\d+)/,(this.productData.special_price*2).toFixed(2))

                            var date = new Date();
                            var dateD = date.getDate();//获取当日
                            var dateY = date.getMonth();//获取当月
                            var dateN =date.getFullYear();//获取当年
                            console.log(dateY,'日期')
                            dateY = dateY+1;
                            console.log(dateD,'获取当日')
                            if(dateD<7){
                                if(dateY==1){
                                    dateY = 12
                                }
                                for(var g=dateD;g>dateD-7;g--){
                                    if(g<=0){
                                        var b = g+30;
                                        dateY = date.getMonth();
                                    }else{
                                        var b = g;
                                    }
                                    this.dateArr.push(dateN+"-"+dateY+"-"+b)

                                }
                            }else{
                                for(var g=dateD;g>dateD-7;g--){
                                    this.dateArr.push(dateN+"-"+dateY+"-"+g)
                                }
                            }

                            console.log(this.dateArr,'数组日期')
                            for(var h=0; h<=this.productData.review.length-1; h++){
                                this.productData.review[h].arrImg = Math.ceil(Math.random()*9);
                                this.productData.review[h].date = Math.ceil(Math.random()*7)-1;

                            }

                            //this.productData.attr =
                            $(document).attr("title",this.productData.name + " - Online Store");// edit title
                            response.data.data.attr.map((item)=>{
                                this.$set(this.productValue,item.k,item.v[0])
                            })
                            if(this.productData.attr == ''){
                                this.propertiesShow = true;
                                this.productValue = {
                                    size:''
                                }
                            }

                            this.featuredProductsList = response.data.data.recommend;
                            this.imgDag = this.productData.picture[0];
                            //判断图片是否有效
                            for(var i=0;i<this.productData.picture.length;i++){
                                await imgError(this.productData.picture[i] , i , this.productData.picture,this,'Array');
                            }

                            this.productNew(this.productData.category);
                            this.titleShow = true;
//                         loadSwiper()
                            $(document).ready(function () {

                            })


                        }else if(response.data.code == 302){
                            window.location.href = response.data.data.url;
                            //console.log(response.data.data.url)
                        }else{
                            this.$toast({message: response.data.message,icon: 'warning-o',});
                            this.productListShow = false;
                        }
                    },function(res){
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                        this.productListShow = false;
                    });
                },
                productNew(value){
                    let that=this;
                    this.productShow = true;
                    this.$http.post('/api/item/randomByKeyword',{num:4,keywords:value},{emulateJSON:true}).then(async function(response){
                        if(response.data.code == 200){
                            this.data = response.data.data;
                            for(var i=0;i<this.data.length;i++){
                                await imgError(this.data[i].thumbnail , i , this.data,this);
                                this.data[i].price_html=this.data[i].price_html.replace(/(\d+.\d+)|(\d+)/,(this.data[i].special_price*2).toFixed(2))
                            }
                            this.productShow = false;

                        }else{
                            this.$toast({message: response.data.message,icon: 'warning-o',});
                            this.productShow = false;
                        }

                    },function(res){
                        this.$toast({message: res.data.message,icon: 'warning-o',});
                        this.productShow = false;
                    });
                },
                cartFn(){
                    // console.log(this.productValue)


                    let data = {
                        id:this.productId,
                        qty:this.productNum,
                        attr: this.productValue,
                    }
                    this.$http.post('/api/cart/add', data, {headers : {Authorization:this.tokenData}},{emulateJSON:true}).then(function(response){
                        if(response.data.code == 200){
                            window.location.href="https://pixulovesk.click/shopping_cart";
                        }else{
                            this.$toast({message: response.data.message,icon: 'warning-o',});
                        }

                    },function(res){
                        this.$toast({message: response.data.message,icon: 'warning-o',});
                    });
                }

            }
        })
    </script>

