    <script>
        $(document).ready(function() {
            var active,links = $('.product_tab-block ul.tabs a');
            active = links.first().addClass('active');
            links.click(function() {
                active.removeClass('active');
                var targetId = $(this).data('target'); // 获取data-target属性的值
                $(this).addClass('active');
                $('.product_tab-block .tabs-content').hide(); // 首先隐藏所有第二个列表的项
                $('#' + targetId).show(); // 然后显示对应的项
                active = $(this);
            });
        });

    </script>
